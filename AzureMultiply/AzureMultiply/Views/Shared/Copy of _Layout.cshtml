<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Dock.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Button.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-1.6.4.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.16.js")" type="text/javascript"></script>
</head>
<body>
    <div class="body-div">
        <div class="top-bar">
            <div id="dock">
        <div class="left"></div>
        <ul>
            <li>
                <a href="http://jquery.com">Multiply</a>
            </li>
            <li style="padding-right: 12px;">
                <a href="http://plugins.jquery.com/">Divide</a>
            </li>
            <li>
                <a href="http://plugins.jquery.com/">Bonds</a>
            </li>
            <li>
                <a href="http://docs.jquery.com/Donate">Logon</a>
            </li>
        </ul>
        <div class="right"></div>
    </div>
        </div>
        <div class="second-bar"><div class="title">Azure Math</div></div>
        <div class="page">
            <header>
                <div id="logindisplay">
                    @Html.Partial("_LogOnPartial")
                </div>
                <nav>
                    <ul id="menu">
                        <li>@Html.ActionLink("Home", "Index", "Home")</li>
                        <li>@Html.ActionLink("About", "About", "Home")</li>
                    </ul>
                </nav>
            </header>
            <section id="main">
                @RenderBody()
            </section>
            <footer>
            </footer>
        </div>
    </div>
</body>
</html>
